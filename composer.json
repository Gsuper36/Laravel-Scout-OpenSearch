{
    "name": "cloudmediasolutions/laravel-scout-opensearch",
    "description": "OpenSearch engine for Laravel Scout",
    "keywords": [
        "laravel",
        "scout",
        "search",
        "opensearch",
        "aws"
    ],
    "require": {
        "php": ">=8.0",
        "laravel/scout": "^8.0 || ^9.1",
        "ongr/elasticsearch-dsl": "^7.2",
        "opensearch-project/opensearch-php": "^2.0",
        "illuminate/support": "^9.17"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^7.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "DaniÃ«l van der Tang",
            "email": "daniel@cloudmedia.solutions"
        }
    ],
    "autoload": {
        "psr-4": {
            "CloudMediaSolutions\\LaravelScoutOpenSearch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
